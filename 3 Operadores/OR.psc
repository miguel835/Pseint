Algoritmo OR
//	Operador lógico OR
	Definir a, b Como Logico
	a <- Verdadero
	b <- Falso
	
	Escribir "OPERADOR LÓGICO OR"
	Escribir "VERDADERO O VERDADERO ES: ", a O a 
	Escribir "VERDADERO O FALSO ES: ", a O b
	Escribir "FALSO O VERDADERO ES: ", b O a 
	Escribir "FALSO O FALSO ES: ", b O b 
	
FinAlgoritmo
